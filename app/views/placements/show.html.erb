
<div class="container-fluid">
	<!-- Placement <%=@placement.id%></br>
	Option <%=@placement.option%></br>

	Video: <%=@video.volume_hash.to_s%>
	<hr>
	<%=@video.volume_hash["video_assets"].class%>
	<hr>
	<%=@embed_codes.to_s%>
	 -->
	

	<%if @placement.option == 1 %>
		<%@video.once_id = @embed_codes["BrightcoveAsset"]%>
