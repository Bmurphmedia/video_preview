	<%end%>


	<%if @placement.option == 2 %>
		<%@video.youtube_id = @embed_codes["YoutubeAsset"]%>
		<%= render "videos/youtube"%>
	<%end%>

	<%if @placement.option == 3 %>
		<%@video.ooyala_id = @embed_codes["OoyalaAsset"]%>
		<%= render "videos/ooyala"%>
	<%end%>

	<%if @placement.option == 4 %>
		<%@video.ooyala_id = @embed_codes["OoyalaAsset"]%>
		<%= render "videos/ooyala_cine1"%>
	<%end%>
	<%if @placement.option == 5 %>
		<%@video.once_id = @embed_codes["BrightcoveAsset"]%>
		<%= render "videos/once_cine"%>
	<%end%>
	<%if @placement.option == 6 %>
		<%@video.once_id = @embed_codes["BrightcoveAsset"]%>
		<%= render "videos/perform_cine1"%>
	<%end%>
	<%if @placement.option == 7 %>
		<%@video.once_id = @embed_codes["BrightcoveAsset"]%>
