<!DOCTYPE html>
<html>
<head>
  <title>VideoPreview</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class = "container">
    

    <!-- Nav Bar -->
    
    <%= render 'layouts/navbar' unless @disable_nav %>
    <!-- end nav bar -->

  </div>

    <!-- Fixed container for flash messages and yield -->
    <div class="container">
      <div class = "row">
        
     

        <div class="col-md-10">
          <% flash.each do |key, value| %>
              <div class="alert" alert-"<%= key %>"><%= value %></div>
          <% end %>
      

          <%= yield %>
        </div>

    </div>
  </div>

</body>
</html>
